{
    "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
    "version": "0.2",
    "language": "en",
    "words": [
        "axisymmetric",
        "blit",
        "eigh",
        "mgrid",
        "ndigits",
        "nolegend",
        "uncast",
        "verts",
        "zlim",
        "zorder"
    ],
    "ignoreWords": [
        "Phonopy",
        "Wannier",
        "pytest",
        "pyright",
        "pydata",
        "pylint",
        "pydocstyle",
        "numpy",
        "isort",
        "xlabel",
        "ylabel",
        "zlabel",
        "xlim",
        "ylim",
        "rtol",
        "ndarray",
        "twinx",
        "scipy",
        "mathbf",
        "pycache",
        "venv",
        "vdot",
        "tqdm",
        "eigvals",
        "monic",
        "mplot",
        "bargmann",
        "imshow",
        "cmap",
        "kron",
        "dicke",
        "spinor",
        "toarray",
        "tocsr",
        "Radau",
        "LSODA",
        "xticks",
        "yticks",
        "xticklabels",
        "yticklabels",
        "vmin",
        "vmax"
    ],
    "userWords": [
        "Larmor",
        "eigenstates"
    ],
    "flagWords": [
        "hte",
        "teh"
    ],
    "allowCompoundWords": true,
    "maxNumberOfProblems": 1000,
    "ignorePaths": [
        "*.snap",
        "node_modules",
        "__snapshots__",
        "package-lock.json",
        "coverage/**",
        "dist/**",
        "dist.*/**",
        "temp/**",
        "CHANGELOG.md",
        "package.json",
        ".cspell.json",
        "cspell.json",
        ".vscode/**",
        ".devcontainer/**",
        ".github/**",
        ".pytest_cache/**",
        ".ruff_cache/**",
        ".venv/**",
        ".vscode/**",
        "build/**",
        "docs/**",
        "*.egg-info/**"
    ],
    "noConfigSearch": true
}
